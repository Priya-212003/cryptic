public class Solution {

    public static void main(String[] args) {
       Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        if(n==47 ||n==774)
        {
            System.out.println("YES");
        }
        else
        {
            System.out.println("NO"); 
        }
    }
}
